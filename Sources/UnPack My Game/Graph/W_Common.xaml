<Window x:Class="UnPack_My_Game.Graph.W_Common"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UnPack_My_Game.Graph"
        mc:Ignorable="d"
        Title="{Binding Title, FallbackValue=Title}" Height="450" Width="800" WindowState="Maximized" WindowStartupLocation="CenterScreen">
    <Grid>
        <Frame NavigationUIVisibility="Hidden" Content="{Binding ActivePage, Mode=OneWay}"/>
    </Grid>
</Window>
