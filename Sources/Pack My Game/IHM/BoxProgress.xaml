        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pack_My_Game.IHM"
        mc:Ignorable="d"
        Title="BoxProgress" Height="450" Width="800">
    <Grid>
        <ProgressBar x:Name="progressBar1" HorizontalAlignment="Left" Height="10" Margin="327,96,0,0" VerticalAlignment="Top" Width="100"/>
        <Button IsEnabled="{Binding Submit}" Content="Button" HorizontalAlignment="Left" Margin="23,46,0,0" VerticalAlignment="Top" Click="button1_Click"/>

    </Grid>
</Window>
